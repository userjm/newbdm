<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <!--
    <Button
        android:id="@+id/start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/result"
        android:layout_alignParentBottom="true"
        android:text="Button" />
    <CheckBox
        android:id="@+id/CB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/start"
        android:layout_alignBottom="@+id/start"
        android:layout_alignParentRight="true"
        android:layout_marginRight="53dp"
        android:text="CheckBox" />
    -->

    <TextView
        android:id="@+id/result"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:lines="7"
        android:text="dfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdddafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsddfsdfdafdsfsd" />

    <FrameLayout
        android:id="@+id/frameLayout1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/result" >

        <com.baidu.mapapi.map.MapView
            android:id="@+id/bmapView"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:clickable="true" />

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <Button
                android:id="@+id/start"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:text="on" />

            <View
                android:id="@+id/jiao"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_above="@+id/start"
                android:layout_alignParentRight="true"
                android:layout_marginRight="18dp"
                android:background="#66048a9f" />

            <CheckBox
                android:id="@+id/CB"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:layout_alignTop="@+id/re"
                android:background="#667dd77d"
                android:text="CB" />

            <View
                android:id="@+id/wei"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignTop="@+id/re"
                android:layout_marginRight="18dp"
                android:layout_toLeftOf="@+id/re"
                android:background="#66048a9f" />

            <View
                android:id="@+id/re"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_above="@+id/start"
                android:layout_marginRight="14dp"
                android:layout_toLeftOf="@+id/jiao"
                android:background="#66048a9f" />
        </RelativeLayout>
    </FrameLayout>

    <View
        android:id="@+id/startstart"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignTop="@+id/result"
        android:layout_marginLeft="16dp"
        android:layout_toRightOf="@+id/result"
        android:background="#66048a9f" />

    <View
        android:id="@+id/View01"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignRight="@+id/frameLayout1"
        android:layout_alignTop="@+id/startstart"
        android:background="#66048a9f" />

    <View
        android:id="@+id/View04"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignBottom="@+id/result"
        android:layout_alignRight="@+id/frameLayout1"
        android:background="#66048a9f" />

    <View
        android:id="@+id/View02"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/startstart"
        android:layout_below="@+id/startstart"
        android:layout_marginTop="15dp"
        android:background="#66048a9f" />

    <View
        android:id="@+id/View03"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/View04"
        android:layout_alignTop="@+id/View02"
        android:background="#66048a9f" />

    <View
        android:id="@+id/View05"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_above="@+id/frameLayout1"
        android:layout_alignLeft="@+id/View02"
        android:background="#66048a9f" />

</RelativeLayout>